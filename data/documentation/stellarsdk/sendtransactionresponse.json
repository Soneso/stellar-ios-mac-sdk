{"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"SendTransactionResponse"}],"platforms":["macOS"],"languages":["swift"]}]},{"kind":"content","content":[{"level":2,"anchor":"overview","text":"Overview","type":"heading"},{"inlineContent":[{"type":"text","text":"After calling sendTransaction, this response indicates whether the transaction"},{"type":"text","text":" "},{"type":"text","text":"was accepted for processing. Note that acceptance does not mean the transaction"},{"type":"text","text":" "},{"type":"text","text":"has executed - you must poll with getTransaction to check final status."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Status values:"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"PENDING: Transaction accepted and waiting to be included in a ledger"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"DUPLICATE: Transaction already submitted (same hash exists)"}]}]},{"content":[{"inlineContent":[{"text":"TRY_AGAIN_LATER: Server is busy, retry the submission","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"ERROR: Transaction rejected (check error field for details)"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"text":"After receiving a PENDING status, use getTransaction with the returned","type":"text"},{"text":" ","type":"text"},{"text":"transactionId (hash) to poll for completion.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Example:"}],"type":"paragraph"},{"code":["let sendResponse = await server.sendTransaction(transaction: signedTx)","switch sendResponse {","case .success(let result):","    switch result.status {","    case SendTransactionResponse.STATUS_PENDING:","        print(\"Transaction submitted: \\(result.transactionId)\")","        \/\/ Poll for status with getTransaction","    case SendTransactionResponse.STATUS_ERROR:","        print(\"Transaction rejected: \\(result.error?.message ?? \"unknown\")\")","    default:","        print(\"Status: \\(result.status)\")","    }","case .failure(let error):","    print(\"RPC error: \\(error)\")","}"],"syntax":"swift","type":"codeListing"},{"inlineContent":[{"type":"text","text":"See also:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"text","text":"[SorobanServer.sendTransaction] for submitting transactions"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"[GetTransactionResponse] for polling transaction status","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"identifier":"https:\/\/developers.stellar.org","isActive":true,"type":"reference"}]}]}],"type":"unorderedList"}]}],"relationshipsSections":[{"type":"inheritsFrom","identifiers":["doc:\/\/stellar-ios-mac-sdk.stellarsdk\/objc(cs)NSObject"],"kind":"relationships","title":"Inherits From"},{"type":"conformsTo","identifiers":["doc:\/\/stellar-ios-mac-sdk.stellarsdk\/objc(pl)NSObject","doc:\/\/stellar-ios-mac-sdk.stellarsdk\/s7CVarArgP","doc:\/\/stellar-ios-mac-sdk.stellarsdk\/s28CustomDebugStringConvertibleP","doc:\/\/stellar-ios-mac-sdk.stellarsdk\/s23CustomStringConvertibleP","doc:\/\/stellar-ios-mac-sdk.stellarsdk\/Se","doc:\/\/stellar-ios-mac-sdk.stellarsdk\/SQ","doc:\/\/stellar-ios-mac-sdk.stellarsdk\/SH"],"kind":"relationships","title":"Conforms To"}],"metadata":{"roleHeading":"Class","role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"SendTransactionResponse","kind":"identifier"}],"symbolKind":"class","externalID":"c:@M@stellarsdk@objc(cs)SendTransactionResponse","modules":[{"name":"stellarsdk"}],"title":"SendTransactionResponse","navigatorTitle":[{"text":"SendTransactionResponse","kind":"identifier"}]},"sections":[],"variants":[{"paths":["\/documentati