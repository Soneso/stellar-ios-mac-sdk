{"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/stellar-ios-mac-sdk.stellarsdk\/documentation\/stellarsdk\/EventSource"},"variants":[{"paths":["\/documentation\/stellarsdk\/eventsource"],"traits":[{"interfaceLanguage":"swift"}]}],"schemaVersion":{"patch":0,"minor":3,"major":0},"sections":[],"primaryContentSections":[{"declarations":[{"platforms":["macOS"],"tokens":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"EventSource","kind":"identifier"}],"languages":["swift"]}],"kind":"declarations"},{"kind":"content","content":[{"text":"Overview","type":"heading","level":2,"anchor":"overview"},{"inlineContent":[{"type":"text","text":"This class implements a client for the Server-Sent Events protocol, which enables"},{"type":"text","text":" "},{"type":"text","text":"servers to push real-time updates to clients over HTTP. The implementation is used"},{"type":"text","text":" "},{"type":"text","text":"throughout the SDK to stream live data from Stellar Horizon servers, including"},{"type":"text","text":" "},{"type":"text","text":"ledgers, transactions, operations, effects, and account changes."}],"type":"paragraph"},{"text":"Protocol Overview","type":"heading","level":2,"anchor":"Protocol-Overview"},{"inlineContent":[{"text":"Server-Sent Events provide a unidirectional channel from server to client over HTTP.","type":"text"},{"text":" ","type":"text"},{"text":"The connection remains open, and the server pushes updates as formatted text messages.","type":"text"},{"text":" ","type":"text"},{"text":"Key features include:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"text":"Automatic reconnection with configurable retry intervals","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Event identification for resuming from last received message","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Named events for routing different message types"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"type":"text","text":"Long-lived connections with appropriate timeouts"}],"type":"paragraph"}]}],"type":"unorderedList"},{"text":"Usage Example","type":"heading","level":2,"anchor":"Usage-Example"},{"type":"codeListing","code":["\/\/ Stream ledger updates from Horizon","let url = \"https:\/\/horizon.stellar.org\/ledgers?cursor=now\"","let eventSource = EventSource(url: url)","","eventSource.onOpen { response in","    print(\"Connection opened, status: \\(response?.statusCode ?? 0)\")","}","","eventSource.onMessage { id, event, data in","    print(\"Received message - ID: \\(id ?? \"none\"), Event: \\(event ?? \"none\")\")","    \/\/ Parse data as JSON ledger response","    if let ledgerData = data?.data(using: .utf8) {","        \/\/ Process ledger data","    }","}","","eventSource.onError { error in","    print(\"Connection error: \\(error?.localizedDescription ?? \"unknown\")\")","}","","\/\/ Close when done","eventSource.close()"],"syntax":"swift"},{"text":"Event Handlers","type":"heading","level":2,"anchor":"Event-Handlers"},{"inlineContent":[{"text":"The EventSource supports three types of callbacks:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"code":"onOpen","type":"codeVoice"},{"type":"text","text":": Called when connection is established"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"onMessage"},{"text":": Called for each message received","type":"text"}]}]},{"content":[{"inlineContent":[{"code":"onError","type":"codeVoice"},{"type":"text","text":": Called when errors occur"}],"type":"paragraph"}]}],"type":"unorderedList"},{"inlineContent":[{"type":"text","text":"Additionally, named event listeners can be registered for specific event types:"}],"type":"paragraph"},{"type":"codeListing","code":["eventSource.addEventListener(\"ledger-update\") { id, event, data in","    \/\/ Handle ledger-specific events","}"],"syntax":"swift"},{"text":"Automatic Reconnection","type":"heading","level":2,"anchor":"Automatic-Reconnection"},{"inlineContent":[{"type":"text","text":"The implementation automatically reconnects on connection failures using an"},{"type":"text","text":" "},{"type":"text","text":"exponential backoff strategy. The retry interval can be set by the server"},{"type":"text","text":" "},{"type":"text","text":"using the "},{"type":"codeVoice","code":"retry:"},{"type":"text","text":" field in the event stream, defaulting to 3000 milliseconds."}],"type":"paragraph"},{"text":"Last Event ID","type":"heading","level":2,"anchor":"Last-Event-ID"},{"inlineContent":[{"type":"text","text":"To support reliable streaming, the last received event ID is persisted and"},{"type":"text","text":" "},{"type":"text","text":"sent with reconnection requests using the "},{"type":"codeVoice","code":"Last-Event-Id"},{"type":"text","text":" header. This allows"},{"type":"text","text":" "},{"type":"text","text":"the server to resume streaming from where it left off."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"See also:"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"reference","isActive":true,"identifier":"https:\/\/html.spec.whatwg.org\/multipage\/server-sent-events.html"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"isActive":true,"identifier":"https:\/\/developers.stellar.org","type":"reference"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"[StreamingHelper] for simplified Horizon streaming integration"}]}]}],"type":"unorderedList"}]}],"metadata":{"navigatorTitle":[{"text":"EventSource","kind":"identifier"}],"externalID":"c:@M@stellarsdk@objc(cs)EventSource","title":"EventSource","modules":[{"name":"stellarsdk"}],"role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"EventSource","kind":"identifier"}],"roleHeading":"Class","symbolKind":"class"},"hierarchy":{"paths":[["